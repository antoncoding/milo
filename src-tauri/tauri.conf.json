{
  "$schema": "https://schema.tauri.app/config/v1",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Milo",
  "mainBinaryName": "Milo",
  "version": "0.1.0",
  "identifier": "com.milo.dev",
  "plugins": {},
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "Milo Settings",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "visible": false,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "useHttpsScheme": true
      }
    ],
    "withGlobalTauri": false,
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "csp": null
    }
  }
}