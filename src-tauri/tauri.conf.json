{
  "$schema": "https://schema.tauri.app/config/v1",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Milo",
  "mainBinaryName": "Milo",
  "version": "0.1.8",
  "identifier": "com.milo.dev",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://raw.githubusercontent.com/antoncoding/milo/main/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEVEN0Q0OEYzNDgyOTIxQUEKUldTcUlTbEk4MGg5N1VITktmZ3VBK2QrVzcrRzJYTmhSVTQ0ZjRvSVFxUGsvcmtEdHE0SXhKemIK"
    }
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "Milo Settings",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "visible": false,
        "skipTaskbar": false,
        "useHttpsScheme": true,
        "alwaysOnTop": false,
        "center": true
      }
    ],
    "withGlobalTauri": false,
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "csp": null
    }
  }
}